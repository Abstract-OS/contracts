{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GovernanceDetails",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "monarchy"
      ],
      "properties": {
        "monarchy": {
          "type": "object",
          "required": [
            "monarch"
          ],
          "properties": {
            "monarch": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "multi_signature"
      ],
      "properties": {
        "multi_signature": {
          "type": "object",
          "required": [
            "members",
            "threshold_votes",
            "total_members"
          ],
          "properties": {
            "members": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "threshold_votes": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            },
            "total_members": {
              "type": "integer",
              "format": "uint8",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "token_weighted"
      ],
      "properties": {
        "token_weighted": {
          "type": "object",
          "required": [
            "token_addr"
          ],
          "properties": {
            "token_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
