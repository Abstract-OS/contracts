    "creation_fee",
    "memory_contract",
    "module_factory_address",
    "next_os_id",
    "owner",
    "version_control_contract"
  ],
  "properties": {
    "creation_fee": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "memory_contract": {
      "type": "string"
    },
    "module_factory_address": {
      "type": "string"
    },
    "next_os_id": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "owner": {
      "type": "string"
    },
    "version_control_contract": {
      "type": "string"
    }
  }
}
