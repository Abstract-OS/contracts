{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "description": "Call to add a new version and code-id for a module Only Admin can call this",
      "type": "object",
      "required": [
        "add_code_id"
      ],
      "properties": {
        "add_code_id": {
          "type": "object",
          "required": [
            "code_id",
            "module",
            "version"
          ],
          "properties": {
            "code_id": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "module": {
              "type": "string"
            },
            "version": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Remove some version of a module",
      "type": "object",
      "required": [
        "remove_code_id"
      ],
      "properties": {
        "remove_code_id": {
          "type": "object",
          "required": [
            "module",
            "version"
          ],
          "properties": {
            "module": {
              "type": "string"
            },
            "version": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Add a new OS to the deployed OSs Only Factory can call this",
      "type": "object",
      "required": [
        "add_os"
      ],
      "properties": {
        "add_os": {
          "type": "object",
          "required": [
            "os_id",
            "os_manager_address"
          ],
          "properties": {
            "os_id": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            },
            "os_manager_address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Remove an OS from the deployed OSs",
      "type": "object",
      "required": [
        "remove_os"
      ],
      "properties": {
        "remove_os": {
          "type": "object",
          "required": [
            "os_id"
          ],
          "properties": {
            "os_id": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "set_admin"
      ],
      "properties": {
        "set_admin": {
          "type": "object",
          "required": [
            "new_admin"
          ],
          "properties": {
            "new_admin": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "set_factory"
      ],
      "properties": {
        "set_factory": {
          "type": "object",
          "required": [
            "new_factory"
          ],
          "properties": {
            "new_factory": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
